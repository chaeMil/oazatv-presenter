{
  "name": "oh-presenter",
  "version": "0.0.1",
  "description": "",
  "author": "ch√§eMil",
  "license": "ISC",
  "main": "./app/server/main.js",
  "scripts": {
    "postinstall": "npm install fabric@2.3.6 --no-optional --no-save",
    "start": "cross-os exec ./app/server/main",
    "canvas": "cross-os exec ./app/canvas/main",
    "pack": "build --dir",
    "dist": "build"
  },
  "cross-os": {
    "exec": {
      "linux": "/usr/local/bin/electron",
      "darwin": "electron",
      "win32": "electron"
    }
  },
  "build": {
    "appId": "cz.chaemil.oh-presenter",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "a-color-picker": "^1.0.6",
    "ajv": "^6.5.3",
    "draggable": "^4.2.0",
    "electron-photon": "^0.1.0",
    "electron-window-state": "^4.1.1",
    "eventbusjs": "^0.2.0",
    "font-awesome": "^4.7.0",
    "font-list": "^1.1.0",
    "fs-extra": "^7.0.0",
    "hotkeys-js": "^3.3.5",
    "ionicons": "^3.0.0",
    "ip": "^1.1.5",
    "jquery": "^3.3.1",
    "json-socket": "^0.3.0",
    "knockout": "^3.4.2",
    "mkdirp": "^0.5.1",
    "node-simple-router": "^0.10.1",
    "npm": "^6.4.0",
    "open-iconic": "^1.1.1"
  },
  "devDependencies": {
    "cross-os": "^1.3.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-prebuilt-compile": "^2.0.7"
  }
}
