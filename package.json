{
  "name": "oh-presenter",
  "version": "0.0.1",
  "description": "",
  "author": "ch√§eMil",
  "license": "ISC",
  "scripts": {
    "postinstall": "install-app-deps",
    "server": "./node_modules/.bin/electron ./app/server/main",
    "canvas": "./node_modules/.bin/electron ./app/canvas/main",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "cz.chaemil.oh-presenter",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "electron-photon": "^0.1.0",
    "electron-window-state": "^4.1.1",
    "fabric": "^2.2.3",
    "font-awesome": "^4.7.0",
    "ip": "^1.1.5",
    "jquery": "^3.3.1",
    "json-socket": "^0.3.0",
    "npm": "^6.0.0"
  },
  "devDependencies": {
    "electron": "^1.8.6",
    "electron-builder": "^20.11.1",
    "electron-prebuilt-compile": "^1.8.4"
  }
}
